{
  "name": "vcf-telegram-bot",
  "description": "Telegram bot to convert numbers to VCF contact file.",
  "repository": "https://github.com/yourusername/vcf-telegram-bot",
  "keywords": ["telegram", "vcf", "bot"],
  "env": {
    "BOT_TOKEN": {
      "description": "Your Telegram Bot Token",
